<html>
<head>
  <title>Profile</title>
  <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
  <link href="//cdnjs.cloudflare.com/ajax/libs/semantic-ui/0.16.1/css/semantic.min.css" rel="stylesheet">
  

  <link href="//cdnjs.cloudflare.com/ajax/libs/semantic-ui/0.16.1/fonts/basic.icons.woff">
   <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
   <script src="//cdnjs.cloudflare.com/ajax/libs/skrollr/0.6.26/skrollr.min.js"></script>
  
  <script src="//cdnjs.cloudflare.com/ajax/libs/semantic-ui/0.16.1/javascript/semantic.min.js"></script>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>

</head>
<body>
  <div class="ui fluid four item menu">
  <a class="active item">
    <i class="home icon"></i> Home
  </a>
  <a href="/contact/" class="item">
    <i class="mail icon"></i> Contact Me
  </a>
  <a href="/works/" class="item">
    <i class="user icon"></i> Websites
  </a>
  <a href="/animated/" class="item">
    <i class="user icon"></i> Other Work
  </a>
</div>
<%= yield %>
    <script type="text/javascript">
    var s = skrollr.init();
    </script>
</body>
</html>
